                  handler: function(direction) {
                    if (direction == 'down' && !recommendation_waypoint_check) {
                      $.ajax({
                        url: kendo.format('/{0}/{1}/remote/recommendations?language=pt-BR', 'movie', '656663-jackass-forever'),
                        type: 'GET',
