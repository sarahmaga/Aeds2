    });

    var watch_url_template = "/movie/644495-house-of-gucci/watch?translate=false&locale={0}&language=pt-BR";
    $("#ott_country_filter").kendoDropDownList({
      filter: "contains",
      height: 250,
      autoWidth: true,
      value: 'BR',
      dataValueField: "country_code",
      dataSource: ott_country_datasource,
      valueTemplate: '<span class="dropdown_flag" style="background-image: url(\'#:data.flag#\')"></span><span>#:data.native_name#</span>',
      template: '<span class="dropdown_flag" style="background-image: url(\'#:data.flag#\')"></span><span>#: data.native_name #</span>',
      change: function(e) {
        window.location = kendo.format(watch_url_template, this.value());
      },
      filtering: function(e) {
        var filterValue = e.filter != undefined ? e.filter.value : "";
        e.preventDefault();

        this.dataSource.filter({
          logic: "or",
          filters: [
            {
              field: "country_code",
              operator: "contains",
              value: filterValue
            },
            {
              field: "native_name",
              operator: "contains",
              value: filterValue
            }
          ]
        });
      }
    });
  }

  var offersHasLoaded = false;

  $(function() {
    if ( offersHasLoaded ) {
      loadDropDown();
    }

    
      window.history.replaceState({ href: '/movie/644495-house-of-gucci?language=pt-BR', event: 'hideOTTOffers' }, null, '/movie/644495-house-of-gucci?language=pt-BR');

      $('div.ott_offer').on('click', function(e) {
        e.preventDefault();

        $('html, body').animate({ scrollTop: 0 }, 0);
        if ( offersHasLoaded == false ) {
          $('div.ott_offer div.text_wrapper').addClass('loading');

          $.ajax({
            url: '/movie/644495-house-of-gucci/remote/watch?translate=false&locale=BR&language=pt-BR',
            type: 'GET',
            beforeSend: function() {
             activatePageLoader();
            }
          }).fail(function() {
            deactivatePageLoader();
            $('div.ott_offer div.text_wrapper').removeClass('loading');
            showError('There was a problem.');
          }).done(function(response) {
            deactivatePageLoader();
            offersHasLoaded = true;
            $('div.ott_offer div.text_wrapper').removeClass('loading');
            $('#ott_offers_window').html(response);
            observer.observe();

            loadDropDown();
            pushOttState({ href: '/movie/644495-house-of-gucci/watch?language=pt-BR', event: 'showOTTOffers' });
          });
        } else if ( $('#ott_offers_window').hasClass('hidden') ) {
          pushOttState({ href: '/movie/644495-house-of-gucci/watch?language=pt-BR', event: 'showOTTOffers' });
        }
      });

      $('div.header.large div.single_column').on('click', 'ul.filters li a', function(e) {
        $('ul.filters li a').removeClass('selected');
        $(this).addClass('selected');

        var selected_quality = $(this).data('panel');
        var selected_items = $('ul.providers li.' + selected_quality);

        $('ul.providers li').addClass('hide');
        selected_items.removeClass('hide');

        $("ul.providers").each(function(index) {
          var items = $(this).find('.' + selected_quality);
          if (items.length == 0) {
            $(this).closest('div.ott_provider').addClass('hide');
          } else {
            $(this).closest('div.ott_provider').removeClass('hide');
          }
        });
      });
    

    $('div.header.large div.single_column').on('click', '#ott_offers_window a.close', function(e) {
      pushOttState({ href: '/movie/644495-house-of-gucci?language=pt-BR', event: 'hideOTTOffers' });
    });
  });
</script>

