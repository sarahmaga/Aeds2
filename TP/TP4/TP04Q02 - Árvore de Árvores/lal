                  handler: function(direction) {
                    if (direction == 'down' && !recommendation_waypoint_check) {
                      $.ajax({
                        url: kendo.format('/{0}/{1}/remote/recommendations?language=pt-BR', 'movie', '639721-the-addams-family-2'),
