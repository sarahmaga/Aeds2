<script type="text/javascript">
  $('td.none a.subscribe').on('click', function(e) {
    var item_type = $(this).attr('data-type');
    var item_id = $(this).attr('data-id');

    $.ajax({
      url: '/notifications/subscribe_to_group',
      type: 'PUT',
      data: {
        media_type: item_type,
        media_id: item_id
      }
    }).fail(function() {
      showError('Ocorreu um problema.');
    }).done(function(response) {

      if (response.failure) {
        showError('Ocorreu um problema.');
      }

      if (response.success) {
        showSuccess('Subscribed', 'Sucesso');
        $('td.none p.notify').toggleClass('hide');
      }

    });
  });

  $('td.none a.unsubscribe').on('click', function(e) {
    var item_type = $(this).attr('data-type');
    var item_id = $(this).attr('data-id');

    $.ajax({
      url: '/notifications/subscribe_to_group',
      type: 'DELETE',
      data: {
        media_type: item_type,
        media_id: item_id
      }
    }).fail(function() {
      showError('Ocorreu um problema.');
    }).done(function(response) {

      if (response.failure) {
        showError('Ocorreu um problema.');
      }

      if (response.success) {
        showSuccess('Unsubscribed', 'Sucesso');
        $('td.none p.notify').toggleClass('hide');
      }

    });
  });
</script>
                
